@model List<MyPortfolio.DAL.Entites.Skill>

<div class="col-six tab-full right">
    <h3>Programlama Dilleri ve Teknolojiler</h3>

    <ul class="skill-bars">
        @foreach (var item in Model) 
        {
            string class_percent = "progress percent" + item.Value.ToString() ;

            <li>
                <div class="@class_percent"><span>@item.Value</span></div>
                <strong>@item.Title</strong>
            </li>
        }        
    </ul>
</div>